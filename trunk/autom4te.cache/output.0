$as_echo "no" >&6; }
      check_xpm=0
     fi
else
  
      { $as_echo "$as_me:$LINENO: result: yes" >&5
$as_echo "yes" >&6; }
      check_xpm=1
      
    
fi

  if test $check_xpm = 1; then
